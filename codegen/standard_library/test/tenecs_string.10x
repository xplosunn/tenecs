package test

import tenecs.string.hasPrefix
import tenecs.test.UnitTestKit
import tenecs.test.UnitTestRegistry
import tenecs.test.UnitTests

hasPrefixTests := implement UnitTests {
  public tests := (registry: UnitTestRegistry): Void => {
    registry.test("hasPrefix", (testkit: UnitTestKit): Void => {
      testkit.assert.equal(true, hasPrefix("", ""))
      testkit.assert.equal(true, hasPrefix("a", ""))
      testkit.assert.equal(true, hasPrefix("ab", ""))

      testkit.assert.equal(true, hasPrefix("abc", "a"))
      testkit.assert.equal(true, hasPrefix("abc", "ab"))
      testkit.assert.equal(true, hasPrefix("abc", "abc"))

      testkit.assert.equal(false, hasPrefix("abc", "abcd"))
      testkit.assert.equal(false, hasPrefix("abc", "abd"))
      testkit.assert.equal(false, hasPrefix("abc", "ad"))
      testkit.assert.equal(false, hasPrefix("abc", "d"))
    })
  }
}
